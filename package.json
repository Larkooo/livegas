{
  "name": "livegas",
  "module": "index.ts",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "ts-proto": "^1.167.3"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "web": "bun run --cwd packages/web",
    "grpc": "cargo run grpc",
    "proto": "protoc --plugin=node_modules/ts-proto/protoc-gen-ts_proto packages/proto/src/*.proto --ts_proto_out=packages/proto/bindings --ts_proto_opt=esModuleInterop=true"
  },
  "dependencies": {
    "long": "^5.2.3",
    "rxjs": "^7.8.1"
  }
}